package com.qa.Game;

public class Game {
	private int xPos;
	private int yPos;
	
	public void moveNorth() {
		yPos += 10; 
	}
	
	public void moveSouth() {
		
	}
	public void moveEast() {
		
	}
	
	public void moveWest() {
		
	}
	public int getXPosition() {
		return this.xPos;
	}
	
	public int getYPosition() {
		return this.yPos;
	}
	
