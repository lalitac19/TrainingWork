package com.qa.Library;

public class Books extends Library {

	private int ISBN; 
	private String author; 
	private String publisher;
	
	public Books (int ISBN, String name, String author, String pub) {
		this.setType("Book");
		this.ISBN = ISBN; 
		this.setName(name); 
		this.author = author; 
		this.publisher = pub; 
		this.setID();  
	}

